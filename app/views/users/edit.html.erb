<h1>プロフィール編集ページ</h1>

<%= form_with(model: @user, local: true) do |f| %>

      <div class="form-group">
  <%= f.label :content, '氏名' %>
  <%= f.text_field :name %>
      </div>





      <div class="form-group">
  <%= f.label :content, '年齢' %>
  <%= f.text_field :age %>
      </div>
      
      <div class="form-group">
  <%= f.label :content, '自己紹介' %>
  <%= f.text_field :explain %>
      </div>
  
      <div class="form-group">
  <%= f.label :content, '画像' %>
  <%=  f.file_field :img %>
      </div>
      
        <%= f.submit '編集' %>
            <% end %>